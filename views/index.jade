          img(src="/img/bigcloud.png", data-at2x="/img/bigcloud@2x.png")
        .mid-cloud
          img(src="/img/midcloud.png", data-at2x="/img/midcloud@2x.png")
        .small-cloud
          img(src="/img/smallcloud.png", data-at2x="/img/smallcloud@2x.png")
        .library
          img(src="/img/library-hook.png")
        .crane
          img(src="/img/longcrane.png", data-at2x="/img/longcrane@2x.png")
        .bucket
        .bucket.bucket-two
        .hide-ladder
        .beta
          img(src="/img/beta.png", data-at2x="/img/beta@2x.png")
        .ladder
          img(src="/img/ladder.png", data-at2x="/img/ladder@2x.png")
